<compound kind="n_alkane">
	<compound src="alkane_tail.xml" label="top_tail">
        <alias label="last_part"/>
    </compound>

    <compound-prototype src="alkane_body.xml"/>

    <repeat count="100">
        <compound src="alkane_body.xml" label="body_#">
            <alias label="body"/>
       		<transform>
			    <equivalence my="body.female_port" target="last_part.male_port" />
		    </transform>
            <alias label="last_part"/>
        </compound>
    </repeat>
	<compound src="alkane_tail.xml" label="bottom_tail">
		<transform>
			<equivalence my="bottom_tail.female_port" target="last_part.male_port" />
		</transform>
	</compound>
</compound>

