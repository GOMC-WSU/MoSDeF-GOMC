    <Angle class1="XBZXYW" class2="XBZXYW" class3="XBZXYW" angle="2" k="400"/>
  </HarmonicAngleForce>
  <RBTorsionForce>
    <Proper class1="BDABCD" class2="BDABCD" class3="BDABCD" class4="BDABCD" c0="-4" c1="7" c2="2" c3="-5" c4="0.0" c5="0.0"/>
